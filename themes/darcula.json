		"titleBar.activeForeground": "#2B2B2B",
		"titleBar.inactiveBackground": "#F6F6F6",
		"titleBar.inactiveForeground": "#B2B2B2"
	},
	"tokenColors": [
		{
			"settings": {
				"background": "#2B2B2B",
				"foreground": "#A9B7C6"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"scope": "comment.line",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#808080"
			}
		},
		{
			"scope": [
				"comment",
				"comment.block.preprocessor",
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#629755"
			}
		},
		{
			"scope": [
				"comment.block keyword"
			],
			"settings": {
				"fontStyle": "bold italic",
				"foreground": "#77B767"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CC7832"
			}
		},
		{
			"scope": [
				"constant.other",
				"support.constant.core",
				"support.constant.ext",
				"support.constant.std"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9876AA"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#6897BB"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"name": "TAG elements",
			"scope": [
				"entity.name.tag",
				"entity.tag.apacheconf"
			],
			"settings": {
				"foreground": "#FFC66D"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS attribute, functions",
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"meta.property-value.css support.function",

				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.scss",
				"meta.property-list.scss support.function",
				"entity.name.function.scss"
			],
			"settings": {
				"foreground": "#E8BF6A"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.id.css",
				"source.css.less entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "#E8BF6A"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CC7832"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#9876AA"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9876AA"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#CC7832"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#D0D0FF"
			}
		},
		{
			"scope": "beginning.punctuation.definition.quote.markdown",
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"scope": "beginning.punctuation.definition.list.markdown",
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"scope": [
				// "punctuation.definition.metadata.markdown",
				"string.other.link.title.markdown",
				"string.other.link.description.markdown"
			],
			"settings": {
				"foreground": "#287BDE"
			}
		},
		{
			"scope": [
				"markup.underline.link.markdown",
				"markup.underline.link.image.markdown"
			],
			"settings": {
				"foreground": "#FFC66D"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#E8BF6A"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#6D9CBE"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#A5C261"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#6897BB"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CC7832"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type",
				"storage.modifier",
				"storage.modifier.import.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CC7832"
			}
		},
		{
			"scope": [
				"string",
				"string.tag",
				"string.value"
			],
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#CC7832"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",

				"punctuation.terminator",
				"punctuation.separator.list.comma",
				"punctuation.separator.delimiter"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CC7832"
			}
		},
		{
			"name": "JavaScript string interpolation ${var}",
			"scope": "meta.template.expression.variable",
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"scope": "support.type.property-name",
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.logical"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CC7832"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"fontStyle": "none",
				"foreground": "#A5C261"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#D0D0FF"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"support.function",
				"support.function.any-method",
				"support.function.git-rebase"
			],
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"java.class.entity.name.function",
				"meta.function-call.object"
			],
			"settings": {
				"foreground": "#FFC66D"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"support.function.construct",
				"support.function.builtin.python"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CC7832"
			}
		},
		{
			"name": "Function declarations",
			"scope": "meta.function-call.static",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFC66D"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.class",

				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",

				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",

				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"entity.other.class"
			],
			"settings": {
				"foreground": "#E8BF6A"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.return.type",
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.name.tag.css",
				"entity.name.tag.scss"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CC7832"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"name": "Variable other property: var->Property",
			"scope": [
				"variable.other.property"
			],
			"settings": {
				"foreground": "#CC7832"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS support",
			"scope": [
				"meta.property-value.css support",
				"constant.other.color.rgb-value.hex.css punctuation.definition",

				"meta.property-value.scss support",
				"constant.numeric.color.hex-value.scss punctuation.definition"
			],
			"settings": {
				"fontStyle": "none",
				"foreground": "#A5C261"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS property",
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.html.css",

				"support.type.property-name.scss"
			],
			"settings": {
				"fontStyle": "none",
				"foreground": "#A9B7C6"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS RGB-HEX values",
			"scope": [
				"constant.other.color.hex-value.css",
				"constant.other.color.rgb-value.hex.css",

				"constant.numeric.color.hex-value.scss",
				"meta.at-rule.include.scss variable",
				"meta.property-value.scss variable"
			],
			"settings": {
				"fontStyle": "none",
				"foreground": "#6897BB"
			}
		},
		{
			"name": "PHP tag <?php",
			"scope": [
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CC7832"
			}
		},
		{
			"name": "Python variables",
			"scope": [
				"variable.parameter.function-call.python"
			],
			"settings": {
				"foreground": "#b64213"
			}
		},
		{
			"name": "Python strings",
			"scope": [
				"string.quoted.single.python",
				"string.quoted.multi.python"
			],
			"settings": {
				"foreground": "#a5c44f"
			}
		},
		{
			"name": "Python classes and functions",
			"scope": [
				"entity.name.type.class.python",
				"entity.name.function.python"
			],
			"settings": {
				"fontStyle":"bold",
				"foreground": "#A9B7C6"
			}
		}
	]
}
