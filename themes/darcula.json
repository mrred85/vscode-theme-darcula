		"editorSuggestWidget.selectedBackground": "#424D5F",
		"editorHoverWidget.background": "#3C3F41",
		"editorHoverWidget.border": "#5B5D5F",
		"panel.background": "#2B2B2B",
		"panel.border": "#4D4D4D",
